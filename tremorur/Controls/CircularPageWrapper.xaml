<ContentView x:Class="tremorur.Controls.CircularPageWrapper"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:tremorur">
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.Clip>
            <EllipseGeometry x:Name="EllipseClip"/>
        </Grid.Clip>

        <ContentPresenter x:Name="ContentHolder"/>
    </Grid>
</ContentView>
